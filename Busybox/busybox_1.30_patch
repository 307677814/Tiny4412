+	\s-1INFO_SPEC\s0 := \s-1NH OPTIONS
+	NH\s0 := [via [inet|inet6] \s-1ADDR\s0] [dev \s-1IFACE\s0] [src \s-1ADDR\s0] [onlink]
+	\s-1OPTIONS\s0 := [mtu [lock] \s-1NUM\s0] [advmss [lock] \s-1NUM\s0]
+.IP "\fBiprule\fR" 4
